      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "interval": "2m",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{namespace=~\"$namespace\", app=~\"$app\"}[$__rate_interval])) or vector(0)",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "type": "gauge",
      "title": "‚ùå Error Rate (%)",
      "gridPos": {
        "h": 7,
        "w": 8,
        "x": 8,
        "y": 1
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "interval": "2m",
      "targets": [
        {
          "expr": "100 * ((sum(rate(http_requests_total{namespace=~\"$namespace\", app=~\"$app\", status=~\"5..\"}[$__rate_interval])) or vector(0)) / (sum(rate(http_requests_total{namespace=~\"$namespace\", app=~\"$app\"}[$__rate_interval])) or vector(1)))",
          "refId": "A"
        }
      ]
    },
    {
